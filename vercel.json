{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_SERVER_PORT": "8501",
    "STREAMLIT_BROWSER_GATHER_USAGE_STATS": "false"
  },
  "functions": {
    "app.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
}
